{
	"name": "variable-editor",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"start": "webpack -w",
		"prod": "webpack -p",
		"electron": "cross-env NODE_ENV=development electron .",
		"electronhome": "cross-env NODE_ENV=dev-home electron .",
		"electronprod": "cross-env NODE_ENV=production electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder"
	},
	"build": {
		"appId": "ncs.variable.creator",
		"directories": {
			"buildResources": "./assets",
			"output": "./dist"
		},
		"extraFiles": "./data/*",
		"win": {
			"icon": "assets/icon.ico"
		}
	},
	"author": "Mark McCoid",
	"license": "ISC",
	"description": "Electron version of Variable Editor for Qlikview",
	"dependencies": {
		"lodash": "^4.17.4",
		"uuid": "^3.1.0",
		"x2js": "^3.1.0"
	},
	"devDependencies": {
		"alertifyjs": "^1.8.0",
		"antd": "^2.6.3",
		"axios": "^0.13.0",
		"babel-core": "^6.5.1",
		"babel-loader": "^6.2.2",
		"babel-plugin-import": "^1.1.1",
		"babel-preset-es2015": "^6.5.0",
		"babel-preset-react": "^6.5.0",
		"babel-preset-stage-0": "^6.5.0",
		"body-parser": "^1.17.2",
		"css-loader": "^0.23.1",
		"deep-freeze-strict": "^1.1.1",
		"electron": "^1.6.11",
		"express": "^4.14.0",
		"file-loader": "^0.9.0",
		"file-saver": "^1.3.3",
		"foundation-sites": "^6.2.0",
		"jquery": "^2.2.1",
		"less": "^2.7.1",
		"less-loader": "^2.2.3",
		"moment": "^2.17.1",
		"node-sass": "^4.5.3",
		"npm-run-all": "^3.1.2",
		"react": "^15",
		"react-addons-test-utils": "^15.4.1",
		"react-dom": "^15",
		"react-redux": "^4.4.5",
		"react-router": "^2.0.0",
		"redux": "^3.6.0",
		"redux-form": "^7.0.3",
		"redux-freeze": "^0.1.5",
		"redux-logger": "^2.7.4",
		"redux-promise": "^0.5.3",
		"redux-thunk": "^2.1.0",
		"sass-loader": "^6.0.6",
		"script-loader": "^0.6.1",
